{
  "name": "proyecto",
  "version": "1.0.0",
  "description": "Sistema web de venta de artesan√≠as",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "start": "npm run build && npm run copy-files && node dist/index.js",
    "dev": "nodemon src/index.ts",
    "init": "npm install -D -s",
    "copy-files": "npm run copy-views && npm run copy-public",
    "copy-views": "copyfiles -u 1 \"./src/views/**/*.ejs\" \"dist\"",
    "copy-public": "copyfiles -u 1 \"./src/public/**/*.*\" \"dist\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "memorystore": "^1.6.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.8.0",
    "pg": "^8.8.0",
    "sequelize": "^6.25.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.6",
    "@types/uuid": "^8.3.4",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
